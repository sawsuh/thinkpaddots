--Commands
dots: cd ~/.dots
y: yay
yu: yay -Syu --devel
vtex: vtex(){ zathura "$1".pdf& vim "$1".tex }; vtex 
ff: firefox
cr: chromium
nit: nitrogen
dis: discord
study: urxvt -name stick -geometry 20x1+1584+147 --hold -e zsh -c "setterm -cursor off && python3 .pomodoro.py"
home: cd ~
nmgui: bash ~/.nmgui.sh
tu: git -C ~/.task add .; git -C ~/.task commit -m "updated tasks"; git -C ~/.task push;
td: git -C ~/.task pull
bt: bash ~/.btooth.sh
kbt: bash ~/.kbtooth.sh
--FilestoVim
xr: ~/.Xresources
sxrc: ~/.config/sxhkd/sxhkdrc
bsprc: ~/.config/bspwm/bspwmrc
pbar: ~/.config/polybar/config
rng: ~/.config/ranger/rc.conf
vrc: ~/.vimrc
